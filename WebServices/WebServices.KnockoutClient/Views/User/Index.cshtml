@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th> Employee ID </th>
        <th> FirstName </th>
        <th> LastName </th>
        <th> IsEnabled </th>
        <th> PasswordChangedDate </th>
        <th> ExpiryDate </th>
        <th> PasswordHash </th>
        <th> LogOnName </th>
        <th></th>
    </tr>
    <tbody data-bind="foreach: Users">
        <tr>
            <td data-bind="text: Id"></td>
            <td data-bind="text: FirstName"></td>
            <td data-bind="text: LastName"></td>
            <td data-bind="text: IsEnabled"></td>
            <td data-bind="text: PasswordChangedDate"></td>
            <td data-bind="text: ExpiryDate"></td>
            <td data-bind="text: PasswordHash"></td>
            <td data-bind="text: LogOnName"></td>
            <td>
                <a data-bind="attr: { 'href': '@Url.Action("Delete", "User")/' + Id}"  class="btn btn-danger right">Delete</a>
                <a data-bind="attr: { 'href': '@Url.Action("Edit", "User")/' + Id}"  class="btn btn-success right">Edit</a>
            </td>
        </tr>
    </tbody>
</table>
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/knockout-3.5.0.js"></script>
<script src="~/Scripts/KOScripts/KORead.js"></script>  